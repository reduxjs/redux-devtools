    "base16": "^1.0.0",
    "codemirror": "^5.62.3",
    "color": "^4.0.1",
    "prop-types": "^15.7.2",
    "react-icons": "^4.2.0",
    "react-select": "^4.3.1",
    "redux-devtools-themes": "^1.0.0",
    "simple-element-resize-detector": "^1.3.0"
  },
