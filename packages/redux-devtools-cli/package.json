{
  "name": "@redux-devtools/cli",
  "version": "4.1.0",
  "description": "CLI for remote debugging with Redux DevTools.",
  "homepage": "https://github.com/reduxjs/redux-devtools/tree/master/packages/redux-devtools-cli",
  "bugs": {
    "url": "https://github.com/reduxjs/redux-devtools/issues"
  },
  "license": "MIT",
  "author": "Mihail Diordiev <zalmoxisus@gmail.com> (https://github.com/zalmoxisus)",
  "files": [
    "bin",
    "dist",
    "src",
    "app",
    "index.js",
    "defaultDbOptions.json"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "redux-devtools": "bin/redux-devtools.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reduxjs/redux-devtools.git"
  },
  "scripts": {
    "build": "tsc && ncp ./src/api/schema_def.graphql ./dist/api/schema_def.graphql",
    "start": "node ./bin/redux-devtools.js",
    "start:electron": "node ./bin/redux-devtools.js --open",
    "clean": "rimraf dist",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "prepack": "pnpm run clean && pnpm run build",
    "prepublish": "pnpm run type-check && pnpm run lint && pnpm run test"
  },
  "engines": {
    "node": ">= 18.12.0"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@emotion/react": "^11.11.3",
    "@redux-devtools/app": "^6.0.0",
    "@types/react": "^18.2.55",
    "body-parser": "^1.20.2",
    "chalk": "^5.3.0",
    "cors": "^2.8.5",
    "cross-spawn": "^7.0.3",
    "electron": "^27.3.2",
    "express": "^4.18.2",
    "get-port": "^7.0.0",
    "graphql": "^16.8.1",
    "knex": "^3.1.0",
    "lodash-es": "^4.17.21",
    "minimist": "^1.2.8",
    "morgan": "^1.10.0",
    "open": "^10.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "semver": "^7.6.0",
    "socketcluster-server": "^19.0.1",
    "sqlite3": "^5.1.7",
    "styled-components": "^5.3.11",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/cross-spawn": "^6.0.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/lodash-es": "^4.17.12",
    "@types/minimist": "^1.2.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.17",
    "@types/semver": "^7.5.6",
    "@types/socketcluster-client": "^19.1.0",
    "@types/socketcluster-server": "^19.0.1",
    "@types/styled-components": "^5.1.34",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.3",
    "jest": "^29.7.0",
    "ncp": "^2.0.0",
    "rimraf": "^5.0.5",
    "socketcluster-client": "^19.1.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "typescript": "~5.3.3"
  }
}
