    "jsan": "^3.1.14",
    "object-path": "^0.11.8",
    "prop-types": "^15.8.1",
    "react-icons": "^4.3.1",
    "simple-diff": "^1.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
