    "@rollup/plugin-babel": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "redux": "^4.1.2",
    "rimraf": "^3.0.2",
