{
  "name": "@redux-devtools/extension",
  "version": "3.0.0",
  "description": "Wrappers for Redux DevTools Extension.",
  "homepage": "https://github.com/reduxjs/redux-devtools/tree/master/packages/redux-devtools-extension",
