    "react": "^16.8.4 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.8.4 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "redux": "^3.4.0 || ^4.0.0 || ^5.0.0"
  }
