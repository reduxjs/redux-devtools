{
  "name": "@redux-devtools/inspector-monitor-trace-tab",
  "version": "4.1.1",
  "description": "Submonitor for Redux DevTools inspector to show stack traces.",
  "homepage": "https://github.com/reduxjs/redux-devtools/tree/master/packages/redux-devtools-inspector-monitor-trace-tab",
  "license": "MIT",
  "author": "Mark Erikson <mark@isquaredsoftware.com>",
  "contributors": [
    "Mihail Diordiev <zalmoxisus@gmail.com> (https://github.com/zalmoxisus)"
  ],
  "files": [
    "lib",
    "src"
  ],
  "main": "lib/cjs/StackTraceTab.js",
  "module": "lib/esm/StackTraceTab.js",
  "types": "lib/types/StackTraceTab.d.ts",
  "sideEffects": false,
  "repository": "https://github.com/reduxjs/redux-devtools",
  "scripts": {
    "build": "pnpm run build:cjs && pnpm run build:esm && pnpm run build:types",
    "build:cjs": "babel src --extensions \".ts,.tsx\" --out-dir lib/cjs",
    "build:esm": "babel src --config-file ./babel.config.esm.json --extensions \".ts,.tsx\" --out-dir lib/esm",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rimraf lib",
    "test": "jest",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "prepack": "pnpm run clean && pnpm run build",
    "prepublish": "pnpm run type-check && pnpm run lint && pnpm run test"
  },
  "dependencies": {
    "@babel/code-frame": "^8.0.0-beta.1",
    "@babel/runtime": "^7.28.3",
    "@types/chrome": "^0.1.4",
    "anser": "^2.3.2",
    "html-entities": "^2.6.0",
    "path-browserify": "^1.0.1",
    "react-base16-styling": "workspace:^",
    "source-map": "^0.5.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.3",
    "@babel/eslint-parser": "^7.28.0",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@emotion/react": "^11.14.0",
    "@redux-devtools/core": "workspace:^",
    "@redux-devtools/inspector-monitor": "workspace:^",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/babel__code-frame": "^7.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.18.0",
    "@types/path-browserify": "^1.0.3",
    "@types/react": "^19.1.12",
    "@types/source-map": "0.5.2",
    "jest": "^30.1.1",
    "jest-environment-jsdom": "^30.1.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "redux": "^5.0.1",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.1",
    "typescript": "~5.9.2"
  },
  "peerDependencies": {
    "@emotion/react": "^11.14.0",
