{
  "name": "react-dock",
  "version": "0.4.0",
  "description": "Resizable dockable react component",
  "keywords": [
    "react",
    "reactjs",
    "dock",
    "sidebar"
  ],
  "homepage": "https://github.com/reduxjs/redux-devtools",
  "bugs": {
    "url": "https://github.com/reduxjs/redux-devtools/issues"
