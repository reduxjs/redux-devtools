    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-react": "^7.25.1",
    "jest": "^27.2.0",
    "react": "^16.14.0",
    "react-bootstrap": "^1.6.3",
    "react-dom": "^16.14.0",
    "react-hot-loader": "^4.13.0",
