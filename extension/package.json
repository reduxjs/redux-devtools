    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "immutable": "^4.3.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "pug": "^3.0.3",
    "rimraf": "^5.0.7",
    "selenium-webdriver": "^4.21.0",
    "sinon-chrome": "^3.0.1",
    "ts-jest": "^29.1.4",
    "typescript": "~5.4.5",
