    "@types/lodash": "^4.17.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/styled-components": "^5.1.34",
    "chromedriver": "^124.0.4",
    "cross-env": "^7.0.3",
    "electron": "^30.1.0",
    "esbuild": "^0.21.4",
